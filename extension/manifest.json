{
  "manifest_version": 3,
  "name": "Feeling Forecast",
  "version": "1.0",
  "description": "A browser extension to feel the mood and weather of any place.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["geolocation"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 